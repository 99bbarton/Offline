outputFile:   "cout"
# Alternatively, output to a file...
#outputFile:  "FMtool.out"

trackerCuts: 
{
# cuts B
  minimum_nactive:    20
  maximum_t0err:      1.5
  maximum_fitmomerr:  0.20
  minimum_fitcon:     1.0e-4
}
# See standard alternative tracker cuts at end of this file

momentumCuts:
{
  optimize_pcut_low:  true
  optimize_pcut_high: true  
}

# Time cuts (starts of live time windows)
t0cuts : [765, 770, 775, 780, 785, 790, 795, 800, 805, 810, 815, 820, 825]

# Brown's CE36825 CE data
conversionElectrons:
{
CEdataFiles : 
[
"/mu2e/data/users/mf/CEFilesBrown/CE36825_0_9.root"
]
numberOfGeneratedMuonConversions : 1.0e5
liveGateFraction                 : 1.00
# Explanation of liveGateFraction -- 
# All conversions were generated using the proper time distribution.
# No correction needs to be made for an artificially imposed live time gate.
}

# Brown's DIO36831 DIO data
decayInOrbitElectrons:
{
DIOdataFiles : 
[
"/mu2e/data/users/mf/DIOFilesBrown/DIO36831_0_99.root"    ,
"/mu2e/data/users/mf/DIOFilesBrown/DIO36831_100_199.root" ,
"/mu2e/data/users/mf/DIOFilesBrown/DIO36831_200_299.root" ,
"/mu2e/data/users/mf/DIOFilesBrown/DIO36831_300_399.root"
]
numberOfGeneratedDIOs : 1.0e7
dioTracksWeighted     : true
liveGateFraction      : 1.00
adHocDIOrescaler      : 1.0
}

# Bernsteins's mixed-tag RPC data
radiativePionCaptureElectrons:
{
RPCdataFiles : 
[
"/mu2e/data/users/mf/RPCFilesMixed1tag.root" ,
"/mu2e/data/users/mf/RPCFilesMixed2tag.root" 
]
numberOfGeneratedRPCs            : 2.0e7
extinction                       : 1.0e-10
adHocRPCrescaler                 : 1.0
# Notice that RPC's do not use liveGateFraction
}

# which figure of merit to optimize momentum window:  
#  FC90 sensitivity versus smoothed Punzi sensitivity
useSmoothedPunziFMtoOptimize: false

# Control of verbosity:

verbosity:  
{ 
  signalEfficiency      : true
  backgroundStrength    : true
  progress              : false
  tracksTTreeLocation   : false
  fileNames             : true
  fileEntriesStatistics : false
  dataProperties        : false
  RPClivePionFraction   : false
  spectra               : false
  backgroundSplines     : false 
  allTables             : false 
}

