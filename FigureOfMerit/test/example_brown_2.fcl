# outputFile:   "cout"
# Alternatively, output to a file...
outputFile:  "example_brown_2.out"

Root:
{
  rootFile : "example_brown_2.root"
  cintFile : "example_brown_2.cint"
}


trackerCuts: 
{
# cuts C
  minimum_nactive:    25
  maximum_t0err:      1.0
  maximum_fitmomerr:  0.18
  minimum_fitcon:     1.0e-3
}
# See standard alternative tracker cuts at end of this file

momentumCuts:
{
  optimize_pcut_low:  true
  optimize_pcut_high: true  
}

# Time cuts (starts of live time windows)
t0cuts : [770]

