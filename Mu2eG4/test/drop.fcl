# Drop all information except the saved random number engine state.
#
# $Id: drop.fcl,v 1.2 2011/05/18 20:55:36 onoratog Exp $
# $Author: onoratog $
# $Date: 2011/05/18 20:55:36 $

#include "minimalMessageService.fcl"
#include "standardProducer.fcl"

# Give this job a name.  
process : Drop01

# Start form an empty source
source : 
{ 
  module_type : RootInput
  fileNames   : "data_03.root"
  maxEvents : -1
}

services : 
{
  message : @local::default_message 

}

physics :
{
  producers:
  {  }


  analyzers:
  { }


  e1 : [outfile]

  end_paths      : [e1]

}

outputs:
{

  outfile:
  {
    module_type     : RootOutput
    filename        : "drop_03.root"
    outputCommands  : ["drop *_*_*_*" , "keep edmRNGsnapshots_*_*_*"
  }
}
