BEGIN_PROLOG

    # Look at the bad events from G4.
    skipBadEvents:
    {
      module_type          : KilledEventFilter
      g4ModuleLabel        : g4run
    }

    maptrackerHit:
    {
      module_type          : MapTrackerHitByID
      makerModuleLabel     : makeDcH
    }

    trckRjctByTime:
    {
      module_type           : BkgTrackRejecterByTime
      generatorModuleLabel  : generate
      g4ModuleLabel         : g4run
      makerModuleLabel      : makeDcH
      nsigmaForTimeSel      : 1.960               #-1: use default time window, x>0: x sigma of the fitted time peak  (ex. 1.960, 1.645 ..)
      protonRejecEff        : -1.0                #efficiency of proton rejection, x<=0: disable the proton rejection               (to switch on the proton rejection both protonRejecEff and pulseCut must be on)
      pulseCut              : -0.015              #pulse cut (in MeV) for the proton rejection, x<=0: disable the proton rejection  (to switch on the proton rejection both protonRejecEff and pulseCut must be on)
      ADCSttnPeriod         : -1.0                #periodicity of the Station equipped with ADC, x<=0: no periodicity (all Stations)
      #minHits              : 5
      #doDisplay            : true
      #clickToAdvance       : false
    }


    trackMiniIReco:
    {
      module_type           : ITTrackMiniIReco
    }

    trackReco:
    {
      module_type           : ITTrackReco
      makerModuleLabel      : makeDcH
      tRejecterModuleLabel  : trckRjctByTime
      mapTrackerHitByID     : maptrackerHit
      minClusMultBend       : 6                   #Threshold on the multiplicity of Closest Hit Clusters to identify a potential bending point
      circChi2cut           :12.0                 #Max acceptable value for the Chi2/NDOF for a circle
      #nsigmaForTimeSel      : 1.250               #-1: use default time window, x>0: x sigma of the fitted time peak  (ex. 1.960, 1.645 ..)
      #trackerStepPoints     : tracker
      #minHits               : 5
      doDisplay             : false
      diagLevel             : 0
      elextractModuleLabel  : extractElData
      # Helix fit configuration
        HelixFit :
        {
                minNHit               : 5
                maxniter              : 100
                minlambda             : 0.1
                minzsep               : 0.1
        }
    }

    kalmanFit:
    {
       module_type          : kalmanFitv2
       cutdp                : 100.5
       oneturn              : 0
       debuglevel           : 0
       materialdb           : "KalmanTestsI/test/material_v42.txt"
# Kalman fit configuration for the final track fit
        KalFit :
        {
                maxiter                                 : 10
                debugLevel                              : 0
                material                                : true
                detailmat                               : false
                detailWrSuppmat                         : true
                hiterr                                  : [ 0.0 ]
                seedsmear                               : 1000000
                maxhitchi                               : 3.0
                mingap                                  : 1.0
                weedhits                                : false
                ambiguityStrategy                       : [ 0 ]
                Neutralize                              : false
                initT0                                  : false
                t0Tolerance                             : [ 1.0 ]
                updateT0                                : false
                exacthitturn                            : true
                momcuttoallowextrpl                     : 85.0
        }

    }

    # Look at the hits from G4.
    trackRecoFit:
    {
      module_type           : ITTrackRecoFit
      makerModuleLabel      : makeDcH
      tRejecterModuleLabel  : trckRjctByTime
      mapTrackerHitByID     : maptrackerHit
      diagLevel             : 0
      patternRecoModuleLabel: trackMiniIReco
      materialdb            : "KalmanTestsI/test/material_v42.txt"
      t0use : 0
# Kalman fit configuration for the final track fit
        KalFit :
        {
                maxiter                                 : 10
                debugLevel                              : 0
                material                                : true
                detailmat                               : false
                detailWrSuppmat                         : true
                hiterr                                  : [ 0.0 ]
                seedsmear                               : 1000000
                maxhitchi                               : 3.0
                mingap                                  : 1.0
                weedhits                                : true
                exacthitturn                            : false
                ambiguityStrategy                       : [ 1 ]
                initT0                                  : true
                t0Tolerance                             : [ 1.0 ]
                updateT0                                : true
#                Neutralize                              : false
                momcuttoallowextrpl                     : 85.0
        }
    }




    # Look at the hits from G4.
    extractElData:
    {
      module_type           : ExtractElectronsData
      generatorModuleLabel  : generate
      g4ModuleLabel         : g4run
      makerModuleLabel      : makeDcH
    }

    # Evaluate Pattern Recognition.
    evalPR:
    {
      module_type           : EvalPatternRecognition
      makerModuleLabel      : makeDcH
      elextractModuleLabel  : extractElData
      tRejecterModuleLabel  : trckRjctByTime
      patternRecoModuleLabel: trackMiniIReco
      #doDisplay            : true
      #clickToAdvance       : false
    }


END_PROLOG
