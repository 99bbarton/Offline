inline Trajectory::Trajectory() {}
#ifndef DEFECT_NO_STDLIB_NAMESPACES
using std::cerr;
#endif


inline const HepGeom::Vector3D<double>& Trajectory::Location::direction(void) const {
  if ( ! _dirIsValid ) {
    std::cerr
      << "Ominous warning in Trajectory::Location::direction: "
      << "_dirIsValid is false!" << std::endl;
  }
  return _direction;
}

