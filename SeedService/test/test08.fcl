# Test the seeds service.
#  Check that calls from analyze fail
#
#
# $Id: test08.fcl,v 1.1 2012/02/04 00:12:48 kutschke Exp $
# $Author: kutschke $
# $Date: 2012/02/04 00:12:48 $

#include "minimalMessageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::default_message

  user : {
     SeedService: {
      policy          : "preDefined"
      baseSeed        :     1
      maxSeedsPerJob  :     6
      checkRange      :  true
      verbosity       :     2
      endOfJobSummary :  false

      stest01 : 0

     }
  }

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      testMode      : 3
      instanceNames : [ "a", "b" ]
    }

  }

  e1 : [stest01 ]

  end_paths      : [e1]

}
