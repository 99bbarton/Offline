# Test the seeds service.
#  Check that calls from analyze fail
#
#
# $Id: test08.fcl,v 1.3 2012/02/10 16:26:42 gandr Exp $
# $Author: gandr $
# $Date: 2012/02/10 16:26:42 $

#include "minimalMessageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::default_message

  user : {
     SeedService: {
      policy            : "preDefinedOffset"
      baseSeed          :     1
      maxUniqueEngines  :     6
      checkRange        :  true
      verbosity         :     2
      endOfJobSummary   :  false

      stest01 : 0

     }
  }

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      testMode      : 3
      instanceNames : [ "a", "b" ]
    }

  }

  e1 : [stest01 ]

  end_paths      : [e1]

}
