Import('env')

Import('mu2e_helper')

helper=mu2e_helper()

mainlib = helper.make_mainlib ( [
    'art_Framework_Core',
    'art_Framework_Services_Registry',
    'MF_MessageLogger',
    'fhiclcpp',
    'art_Utilities',
    ] )

helper.make_plugins( [
    mainlib,
    ] )

helper.make_dict_and_map( [
    mainlib,
    ] )    

