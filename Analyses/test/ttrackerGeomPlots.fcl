#include "minimalMessageService.fcl"

# Give this process a name.
process_name : TTrackerGeom

# Read events from a file (made by example 3)
source :
{
  module_type : RootInput
  fileNames : ["data_03.root"]
  maxEvents : 1
}

services : {
  message : @local::default_message

  TFileService : { fileName : "ttrackerGeom.root" }

  user : {
    GeometryService :  { inputFile : "Mu2eG4/test/geom_01.txt" }

    ConditionsService : { conditionsfile : "Mu2eG4/test/conditions_01.txt" }
  }

}

physics : {

  analyzers:
  {
    ttgeom:  {  module_type   : TTrackerGeomIntRootPlots   }
  }

  p1 : []
  e1 : [ttgeom]

  trigger_paths  : [p1]
  end_paths      : [e1]

}



