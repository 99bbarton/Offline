#include "minimalMessageService.fcl"

process_name : DumpDataProducts

source : {
  module_type : RootInput
  fileNames   : @nil
  maxEvents   : 5
}

services : {
  message : @local::default_message
}

physics : {
  e1        : [dumper]
  end_paths : [e1]
}

outputs:  {

 dumper : {
      module_type              : FileDumperOutput
      wantProductFullClassName : true
  }

}
