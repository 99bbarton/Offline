{

  gROOT->Reset();

  gROOT->SetStyle("Plain");

  gStyle->SetOptStat("");

  TFile* file = new TFile( "readMCTrajectories.root");

  TNtuple* nt; file->GetObject("readMCTraj/ntup",nt);

  TCanvas *canvas = new TCanvas("c", "MC Trajectoreis", 900, 900 );

  canvas->Divide(2,2);

  canvas->cd(1); nt->Draw( "x:y:z","evt==1","");
  canvas->cd(2); nt->Draw( "x:y:z","evt==2","");
  canvas->cd(3); nt->Draw( "x:y:z","evt==3","");
  canvas->cd(4); nt->Draw( "x:y:z","evt==4","");


}
