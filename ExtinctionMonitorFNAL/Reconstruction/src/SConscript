import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

my_libs=['mu2e_ExtinctionMonitorFNAL_Geometry',
         'mu2e_RecoDataProducts',
         'mu2e_DataProducts',
         'CLHEP']

mainlib = helper.make_mainlib ( my_libs )
helper.make_plugins( [ mainlib, my_libs ] )

