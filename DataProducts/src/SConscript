import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

mainlib = helper.make_mainlib ( ['cetlib',
				  'mu2e_GeneralUtilities'] )

helper.make_dict_and_map( [ mainlib,
                            'art_Persistency_Common',
                            'art_Utilities',
                            'CLHEP',
                            'cetlib',
                            'Reflex'
                          ] )

