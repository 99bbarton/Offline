import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

mainlib = helper.make_mainlib ( [ 'mu2e_DataProducts',
                                  'btrk_ProbTools',
                                  'art_Framework_Core',
                                  'art_Utilities',
                                  'art_Framework_Services_Registry',
                                  'Reflex',
                                  'Core',
                                ] )

helper.make_plugins( [ mainlib,
                     ] )

helper.make_dict_and_map( [ mainlib,
                          ] )

