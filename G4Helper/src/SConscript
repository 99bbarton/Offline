import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

mainlib = helper.make_mainlib ( [ 'mu2e_GeometryService',
                                  'art_Framework_Core',
                                  'CLHEP' ] )

helper.make_plugins( [ mainlib,
                       'mu2e_GeometryService',
                       'art_Framework_Core',
                       'CLHEP',
                       ] )

